   echo "\n";
   printf("%f",$b);
   echo " ";
   printf("%f",$c);
   echo " ";
   printf("%f",$a);
   }
}
if($c>$b && $c>$a)    
{
   if($b>$a)
   {
   printf("%f",$a);
   echo " ";
   printf("%f",$b);
   echo " ";
   printf("%f",$c);
   echo "\n";
   printf("%f",$c);
   echo " ";
   printf("%f",$b);
